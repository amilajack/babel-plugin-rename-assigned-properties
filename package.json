  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamonkko/babel-rename-assigned-properties/issues"
  },
  "homepage": "https://github.com/jamonkko/babel-rename-assigned-properties#readme",
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.5.0",
    "estraverse-fb": "^1.3.1",
    "isparta": "^4.0.0",
    "lodash": "^4.6.1",
    "mocha": "^2.4.5"
  }
